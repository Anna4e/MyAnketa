         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>QServlet</servlet-name>
        <servlet-class>ua.kiev.prog.QServlet</servlet-class>
    </servlet>
